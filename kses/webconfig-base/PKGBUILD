pkgrel=1
pkgdesc="Guinnux webconfig base"
arch=('any')
license=('GPL')
url="http://www.guinnux.org"
groups=('base')
depends=('lua' 'lighttpd' 'lsqlite3' 'lua-filesystem')
source=(webconfig-base::git+http://gitlab.kses.net/guinnux-apps/${pkgname}.git#tag=v${pkgver})
      
